   "source": [
    "file = open(\"file1.txt\", \"r\")\n",
    "\n",
    "number_of_lines = 0\n",
    "number_of_words = 0\n",
    "for line in file:\n",
    "  line = line.strip(\"\\n\")\n",
    "  words = line.split()\n",
    "  number_of_lines += 1\n",
    "  number_of_words += len(words)\n",
    "\n",
    "file.close()\n",
    "\n",
    "print(\"# of lines : \", number_of_lines, \"# of words:\", number_of_words)"
   ]
